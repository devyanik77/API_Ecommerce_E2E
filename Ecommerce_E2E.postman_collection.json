							"if (jsonData.orders && jsonData.orders.length > 0) {\r",
							"    let orderId = jsonData.orders[0];\r",
							"    pm.collectionVariables.set(\"orderId\", orderId);\r",
							"    console.log(\"Order ID saved:\", orderId);\r",
							"} else {\r",
							"    console.log(\"orders NOT FOUND in response\");\r",
							"}\r",
							"\r",
							"if (jsonData.productOrderId && jsonData.productOrderId.length > 0) {\r",
							"    let prodOrderId = jsonData.productOrderId[0];\r",
